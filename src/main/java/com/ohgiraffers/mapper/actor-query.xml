<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="selectActByFamilyName">
        SELECT
        *
        FROM
        actor
        WHERE
        first_name LIKE CONCAT(?, '%')
   </entry>
    <entry key="insertMenu">
        INSERT INTO
        actor(first_name, last_name)
        VALUES
        (?,?)
    </entry>
    <entry key="updateMenu">
        UPDATE
            actor
        SET
            first_name = ?,
            last_name = ?
        WHERE
            actor_id = ?
    </entry>
    <entry key="deleteMenu">
        DELETE FROM
            actor
        WHERE
            actor_id = ?
    </entry>
</properties>